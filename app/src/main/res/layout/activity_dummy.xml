<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.freya.loginfoodos.DummyActivity"
    android:background="#2c3e50">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="31dp"
        android:id="@+id/textView34"
        android:layout_alignParentEnd="true"
        android:textColor="#2ecc71"
        android:textStyle="normal|bold" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginRight="@dimen/fab_margin_right"
        android:layout_marginBottom="37dp"
        android:src="@drawable/ic_action_name"
        android:elevation="6dp"
        android:id="@+id/fab_call"
        app:pressedTranslationZ="16dp"
        app:backgroundTint="@color/fab_3"

        android:onClick="Call"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginRight="@dimen/fab_margin_right"
        android:src="@drawable/ic_action_whatsapp"
        android:elevation="6dp"
        android:onClick="newchat"
        android:id="@+id/fab_wassup"
        app:pressedTranslationZ="16dp"
        app:backgroundTint="@color/fab_1"

        android:layout_alignTop="@+id/fab_msg"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="19dp" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginRight="@dimen/fab_margin_right"
        android:src="@drawable/ic_action_message"
        android:elevation="6dp"
        android:id="@+id/fab_msg"
        app:pressedTranslationZ="16dp"
        android:onClick="msg"
        app:backgroundTint="@color/fab_2"

        android:layout_alignTop="@+id/fab_call"
        android:layout_centerHorizontal="true" />

    <Button
        android:text="Contact Foodos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button7"
        android:background="#2ecc71"
        android:layout_above="@+id/fab_call"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="60dp"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="notify"
        android:text="Notifications"
        android:id="@+id/button6"
        android:background="#2ecc71"
        android:layout_above="@+id/button7"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="80dp"
        android:layout_alignParentEnd="true" />

</RelativeLayout>
